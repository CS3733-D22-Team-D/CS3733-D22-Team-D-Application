                        <Color opacity="0.15000000596046448" />
                     </color>
                  </DropShadow>
               </effect>
               <children>
                  <HBox alignment="BOTTOM_CENTER" maxHeight="-Infinity" prefHeight="50.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <ToggleButton id="tableToggle" fx:id="createdRequests" mnemonicParsing="false" onAction="#selectCreated" prefHeight="75.0" prefWidth="1000.0" selected="true" styleClass="tableToggle" stylesheets="@../../../../toggleButton.css" text="Created Requests" textFill="WHITE" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                           <toggleGroup>
                              <ToggleGroup fx:id="requestTables" />
                           </toggleGroup>
                        </ToggleButton>
                        <ToggleButton id="tableToggle" fx:id="assignedRequests" mnemonicParsing="false" onAction="#selectAssigned" prefHeight="75.0" prefWidth="1000.0" styleClass="tableToggle" stylesheets="@../../../../toggleButton.css" text="Assigned Requests" textFill="WHITE" toggleGroup="$requestTables">
