                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                                 <columns>
                                    <TableColumn id="col" fx:id="ReqID22" prefWidth="75.0" text="ReqID" />
