                              <Font name="System Bold" size="14.0" />
                           </font>
                        </ToggleButton>
                        <ToggleButton id="tableToggle" fx:id="relevantRequests" mnemonicParsing="false" onAction="#selectRelevant" prefHeight="75.0" prefWidth="1000.0" styleClass="tableToggle" stylesheets="@../../../../toggleButton.css" text="Relevant Requests" textFill="WHITE" toggleGroup="$requestTables">
