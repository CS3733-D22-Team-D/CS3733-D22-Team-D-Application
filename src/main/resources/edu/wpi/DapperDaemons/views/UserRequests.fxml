                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                                 <columns>
                                    <TableColumn id="col" fx:id="ReqID221" prefWidth="75.0" text="ReqID" />
                                    <TableColumn id="col" fx:id="Priority221" prefWidth="75.0" text="Priority" />
                                    <TableColumn id="col" fx:id="RoomID221" prefWidth="75.0" text="RoomID" />
                                    <TableColumn id="col" fx:id="Requester221" prefWidth="75.0" text="Requester" />
                                    <TableColumn id="col" fx:id="Assignee221" prefWidth="75.0" text="Assignee" />
                                    <TableColumn id="col" fx:id="Status221" onEditCommit="#onEditStatus" prefWidth="75.0" text="Status" />
                                 </columns>
                              </TableView>
                           </children>
                        </VBox>
                        <VBox prefHeight="177.0" prefWidth="508.0">
                           <children>
                              <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="24.0" prefWidth="508.0" style="-fx-background-color: #4b5b81;" />
