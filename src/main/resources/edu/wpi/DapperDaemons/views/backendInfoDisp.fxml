<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="550.0" prefWidth="765.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.DapperDaemons.controllers.BackendInfoController">
   <opaqueInsets>
      <Insets top="50.0" />
   </opaqueInsets>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="51.0" prefWidth="305.0" style="-fx-background-color: #4B5B81;">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Patients">
                           <font>
                              <Font name="System Bold" size="23.0" />
                           </font>
                        </Text>
                     </children>
                  </VBox>
                  <TableView id="table" fx:id="patientsTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn id="col" fx:id="PatientID" prefWidth="75.0" text="ID" />
                        <TableColumn id="col" fx:id="PatientFirst" prefWidth="75.0" text="First" />
                        <TableColumn id="col" fx:id="PatientLast" prefWidth="75.0" text="Last" />
                        <TableColumn id="col" fx:id="PatientDOB" prefWidth="75.0" text="DOB" />
                        <TableColumn id="col" fx:id="PatientBT" prefWidth="75.0" text="BloodType" />
