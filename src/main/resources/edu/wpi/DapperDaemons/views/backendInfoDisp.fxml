                  </TableView>
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="51.0" prefWidth="305.0" style="-fx-background-color: #4B5B81;">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Equipment">
                           <font>
                              <Font name="System Bold" size="23.0" />
                           </font>
                        </Text>
                     </children>
                  </VBox>
                  <TableView id="table" fx:id="equipmentTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn id="col" fx:id="EquipmentID" prefWidth="75.0" text="ID" />
                        <TableColumn id="col" fx:id="EquipmentName" prefWidth="75.0" text="Name" />
                        <TableColumn id="col" fx:id="EquipmentType" prefWidth="75.0" text="Type" />
                        <TableColumn id="col" fx:id="EquipmentSerialNum" prefWidth="75.0" text="SerialNum" />
                        <TableColumn id="col" fx:id="EquipmentCleanStatus" prefWidth="75.0" text="CleanStatus" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</HBox>
