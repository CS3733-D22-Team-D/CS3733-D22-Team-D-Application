                     </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="51.0" prefWidth="305.0" style="-fx-background-color: #4B5B81;">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Locations">
                           <font>
                              <Font name="System Bold" size="23.0" />
                           </font>
                        </Text>
                     </children>
                  </VBox>
                  <TableView id="table" fx:id="locationsTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn id="col" fx:id="LocationID" prefWidth="75.0" text="ID" />
                        <TableColumn id="col" fx:id="LocationX" prefWidth="75.0" text="X" />
                        <TableColumn id="col" fx:id="LocationY" prefWidth="75.0" text="Y" />
                        <TableColumn id="col" fx:id="LocationFloor" prefWidth="75.0" text="Floor" />
                        <TableColumn id="col" fx:id="LocationBuilding" prefWidth="75.0" text="Building" />
                        <TableColumn id="col" fx:id="LocationType" prefWidth="75.0" text="Type" />
                        <TableColumn id="col" fx:id="LocationLongName" prefWidth="75.0" text="LongName" />
                        <TableColumn id="col" fx:id="LocationShortName" prefWidth="75.0" text="ShortName" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="51.0" prefWidth="305.0" style="-fx-background-color: #4B5B81;">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Employees">
                           <font>
                              <Font name="System Bold" size="23.0" />
                           </font>
                        </Text>
                     </children>
                  </VBox>
                  <TableView id="table" fx:id="employeesTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn id="col" fx:id="EmployeeID" prefWidth="75.0" text="ID" />
                        <TableColumn id="col" fx:id="EmployeeFirst" prefWidth="75.0" text="First" />
                        <TableColumn id="col" fx:id="EmployeeLast" prefWidth="75.0" text="Last" />
