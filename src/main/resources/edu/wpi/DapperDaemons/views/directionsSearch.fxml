<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER_RIGHT" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="222.0" style="-fx-background-color: #FFFEFE;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.DapperDaemons.map.pathfinder.PathfinderHandler">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="To:">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Text>
            <JFXComboBox fx:id="toLocation" editable="true" labelFloat="true" onKeyTyped="#startFuzzySearch" prefHeight="34.0" promptText="Search for a Location" style="-fx-border-color: #000000; -fx-background-color: #FFFFFF; -fx-background-radius: 7.5; -fx-border-radius: 7.5;">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </padding>
            </JFXComboBox>
         </children>
         <VBox.margin>
            <Insets left="25.0" right="25.0" />
         </VBox.margin>
      </VBox>
      <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="From:">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Text>
            <JFXComboBox fx:id="fromLocation" editable="true" labelFloat="true" onKeyTyped="#startFuzzySearch" prefHeight="50.0" promptText="Search for a Location" style="-fx-border-color: #000000; -fx-background-color: #FFFFFF; -fx-background-radius: 7.5; -fx-border-radius: 7.5;">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </padding>
            </JFXComboBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button mnemonicParsing="false" onMouseClicked="#clearPath" prefHeight="30.0" prefWidth="60.0" style="-fx-background-color: #C4C4C4; -fx-background-radius: 5.5;" text="Clear">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                           <VBox.margin>
                              <Insets right="25.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button mnemonicParsing="false" onMouseClicked="#showPath" prefHeight="30.0" prefWidth="50.0" style="-fx-background-color: #C4C4C4; -fx-background-radius: 5.5;" text="Go">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
         <VBox.margin>
            <Insets left="25.0" right="25.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
